`#include <iostream>
#include <fstream>
#include <iomanip>

const char FR[]="duomenys.txt";
const char FW[]="Akatsukai.txt";

using namespace std;
void Skaito (const char FR[], double a[], int &n);
void Filtravimas (double a[], int n, double &akatsukai,double &akatsukai2);
void Irasymas (const char FW[], int n, double a[], double akatsukai, double akatsukai2);
int main()
{  int n;
   double a[100], akatsukai, akatsukai2;
    Skaito ( FR,a,n);
    Filtravimas(a,n,akatsukai, akatsukai2);
    Irasymas (FW, n, a, akatsukai,akatsukai2);
    return 0;
}

void Skaito (const char FR[], double a[], int &n)
{
   ifstream fr(FR);
  fr>>n;
  for(int i=0; i<n; i++)
  {
       fr>>a[i];
  }
}

void Filtravimas(double a[], int n, double &akatsukai, double &akatsukai2)
{
      akatsukai = a[0]+a[1];
    for(int i=0; i<n; i++)
  {
    if(a[i]+a[i+1]>akatsukai)
    {
        akatsukai=a[i];
        akatsukai2=a[i-1];
    }
  }
  if(akatsukai==0 || akatsukai2==0){
             akatsukai=69;
      }
  cout<<akatsukai;
}
void Irasymas (const char FW[], int n, double a[],double akatsukai, double akatsukai2)
{
   ofstream fw(FW);
         for(int i=0; i<n; i++)
  {
      if(akatsukai==a[i] && akatsukai2==a[i-1] && akatsukai!=69)
      {
          fw<<i+1<<" "<<akatsukai<<" "<<endl;
          fw<<i-1+1<<" "<<akatsukai2<<endl;
      }
  }
    if(akatsukai==69)
    {
        fw<<"neee"<<endl;
    }

}

//Darzas///////////////////////////////////////////////////////////



#include <iostream>
#include <fstream>
#include <iomanip>
#include <algorithm>
#include <string>

const char FR[]="duomenys.txt";
const char FW[]="Akatsukai.txt";

using namespace std;

void Skaito (const char FR[], int a[], int &n);
void display (const char FW[], int a[], int n);
void Skaiciuojame (int a[], int n);
void naikiname (int a[], int n, int &k);
void Iterpiame(int a[], int &n, int k);
void Irasymas (const char FW[], int n, int a[], int min );

int main()
{
 int a[100], n;
 int k=0;
 int min=0;
  Skaito(FR,a,n);
  Skaiciuojame(a,n);
  naikiname(a,n, k);
  Irasymas(FW, n, a,min);
  Iterpiame( a, n,k);
  Irasymas(FW, n, a,min);
  return 0;
}

void Skaito (const char FR[],int a[], int &n)
{
   ifstream fr(FR);
   fr>>n;
  for(int i=0; i<n; i++)
  {
       fr>>a[i];
  }
}

void Skaiciuojame (int a[], int n)
{
   sort(a, a+n);
   display(FW, a, n);
 }
 
void naikiname(int a[], int n, int &k){
    int m;
    for(int i=0; i<n; i++)
  {
    for(int j=i+1; j<n; j++)
       {
           if(a[i]==a[j]){
               //a[i]==a[j];
               m=i;
                a[m]=69;
             k++;
           }
       }
    }
}
    
void display(const char FW[], int a[], int n) {
       ofstream fw(FW);
   for(int i = 0; i < n; ++i)
   {
       fw<< a[i] <<" ";
   }
   fw<<" "<<endl;
} 

void Iterpiame(int a[], int &n, int k)
{
    int g=n;
    for(int i=0; i<g;i++){
        if(a[i]==0){
            for(int t=g;t>i;t--){
            
                a[t+1]=a[t];
            }
            g++;
            a[i+1]=100;
            }
        }
    
    n=g;
    
}

void Irasymas (const char FW[], int n,  int a[], int min)
{
   ofstream fw(FW, ios::app);
    for(int i=0; i<n; i++)
  {
     if(a[i]==69){
       fw<<" ";}
       if(a[i]!=69){
       fw<<a[i]<<" ";}
      
  }
     fw<<" "<<endl;
     cout<<n;
  fw.close();
}
