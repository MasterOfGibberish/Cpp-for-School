#include <fstream>
#include <iostream>
#include <iomanip>

using namespace std;
const char FR[]="U1.txt";
const char FW[]="U1rez.txt";

void Skaiciavimas( int visi[], int suvalge[]);

int main()
{
    int visi[20]= {0}, suvalge[20] = {0}; //suvalge = kiek suvalge vysniu  ar slyvu nepamenu
    Skaiciavimas ( visi, suvalge);  //visi = kiek suvalge pirmos 10 mergaiciu
    return 0;
}

void Skaiciavimas( int visi[], int suvalge[])
{
    ifstream fr(FR);
    ofstream fw(FW);
    for (int i = 0; i < 10; i++)
    {
        fr >> suvalge[i];
        //cout << suvalge[i] << " ";
    }
    fr.close();


   for (int i = 0; i < 20; i++)
   {
       visi[i] = suvalge[i];
   }

   for (int i = 0; i < 10; i++)
   {
       for (int j = 1; j <= 10 - visi[i]; j++)
       {
           suvalge[i + j]++;
       }
   }

    for (int i = 0; i < 20; i++)
    {
        fw << suvalge[i] << " ";
    }
    fw.close();
}
